{
  "name": "reactstarter",
  "private": true,
  "type": "module",
  "version": "0.1.0",
  "scripts": {
    "build": "tsc -b && vite build",
    "c": "git add . && npx lint-staged && run d",
    "deps": "run dep:next 10 && yarn",
    "check:project": "yarn build && yarn lint:all && yarn file:src/modules/utils.spec.ts",
    "start": "yarn deps && yarn check:project && yarn dev",
    "dev": "vite --host",
    "lint:all": "node scripts/tasks/lint/lint-all.js",
    "lint:files": "node scripts/tasks/lint/lint-files.js",
    "test": "vitest run -u --coverage",
    "test:file": "node scripts/tasks/run/run-test.js",
    "test:circular.dependencies": "npx dpdm ./src/*.tsx"
  },
  "lint-staged": {
    "*.js": "npm run lint:files",
    "*.ts": "npm run lint:files",
    "*.tsx": "npm run lint:files",
    "*.css": "npm run lint:files"
  },
  "niketaScripts": {
    "**/*.ts": "yarn test:file",
    "**/*.tsx": "yarn test:file"
  },
  "dependencies": {
    "helpers-fn": "2.0.0",
    "rambdax": "11.3.1",
    "react": "19.2.0-canary-ea05b750-20250408",
    "react-dom": "19.2.0-canary-ea05b750-20250408",
    "react-router-dom": "7.5.2",
    "string-fn": "3.6.3",
    "tailwind-merge": "3.2.0",
    "tailwindcss": "4.1.4"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.0-beta.1",
    "@eslint/js": "10.0.0",
    "@stylistic/eslint-plugin-js": "4.2.0",
    "@stylistic/eslint-plugin-jsx": "4.2.0",
    "@stylistic/eslint-plugin-ts": "4.2.0",
    "@tailwindcss/vite": "4.1.4",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@types/eslint__js": "9.14.0",
    "@types/fs-extra": "11.0.4",
    "@types/node": "22.15.2",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "@typescript-eslint/eslint-plugin": "8.31.1-alpha.2",
    "@typescript-eslint/parser": "8.31.1-alpha.2",
    "@vitejs/plugin-react": "4.4.1",
    "@vitejs/plugin-react-swc": "3.9.0",
    "@vitest/coverage-v8": "3.1.2",
    "eslint": "9.25.1",
    "eslint-plugin-check-file": "3.2.0",
    "eslint-plugin-compat": "6.0.2",
    "eslint-plugin-perfectionist": "4.12.3",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-unused-imports": "4.1.4",
    "fs-extra": "11.3.0",
    "jsdom": "27.0.0-beta.0",
    "lint-staged": "15.5.1",
    "oxlint": "0.16.7",
    "prettier": "4.0.0-alpha.12",
    "stylelint": "16.19.1",
    "stylelint-config-recess-order": "6.0.0",
    "stylelint-config-standard": "38.0.0",
    "stylelint-high-performance-animation": "1.11.0",
    "typescript": "5.9.0-dev.20250425",
    "typescript-eslint": "8.31.1-alpha.2",
    "vite": "6.3.3",
    "vitest": "3.1.2"
  }
}
